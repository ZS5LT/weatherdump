{
  "name": "weatherdump",
  "version": "0.0.0a",
  "engineVersion": "0.0.0a",
  "description": "WeatherDump GUI",
  "main": "electron.prod.js",
  "scripts": {
    "dev": "webpack --config webpack.config.dev.js && node server.dev.js",
    "dev-electron": "electron electron.dev.js",
    "build": "webpack --config webpack.config.prod.js"
  },
  "build": {
    "productName": "WeatherDump",
    "artifactName": "weatherdump-gui-${version}-${os}-${arch}.${ext}",
    "icon": "./resources/icon_by_eucalyp.png",
    "files": [
      "electron.prod.js"
    ],
    "win": {
      "target": [
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "extraFiles": [
      {
        "from": "resources",
        "to": "resources/app/gui",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "../dist/weatherdump-cli-${os}-${arch}",
        "to": "resources/app/engine",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "author": "Open Satellite Project",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "get-port": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "electron": "^4.1.4",
    "gl-react": "^3.15.0",
    "gl-react-dom": "^3.15.0",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-router-transition": "^1.3.0",
    "react-spinners": "^0.5.4",
    "react-websocket": "^2.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "superagent": "^5.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpackbar": "^3.1.5"
  }
}
